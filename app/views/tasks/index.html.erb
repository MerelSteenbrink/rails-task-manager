<h1>YOUR TASKS!!!</h1>
<p>wuuuuuuuuuuuuhuuuu</p>

<ul>
    <% @tasks.each do |task| %>
    <li>
      <%= link_to task.name, task_path(task) %>
      <%= link_to "(edit)", edit_task_path(task)%>
      <%= link_to "[Delete]", task_path(task),
      method: :delete,
      data: { confirm: "Are you sure?" } %>
    </li>
    <% end %>
</ul>
